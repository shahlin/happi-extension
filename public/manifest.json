{
    "name": "Happi",
    "description": "Track your daily developer productivity",
    "version": "1.1.16",
    "manifest_version": 3,
    "author": "Shahlin Ibrahim",
    "icons": {
        "16": "logo/logo-small-16.png",
        "32": "logo/logo-small-32.png",
        "48": "logo/logo-small-48.png",
        "128": "logo/logo-small-128.png"
    },
    "permissions": [
        "identity",
        "identity.email",
        "storage",
        "alarms",
        "notifications"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "js/alarm.js",
        "type": "module"
    }
}